{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "ESNext",
    ],
    "module": "CommonJS",
    "rootDir": "./src",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
  },
  "include": [
    "src/**/*"
, "../../pjs/sketch/src/projects/wasted/font"  ]
}